/**index.wxss**/
@import "../../components/chat-input/chat-input.wxss";

.setting-part{
  position: absolute;
  width: 100%;
  height: 60rpx;
  background-color: #D4E1F6;
}

.setting-part .setting-bot{
  margin: 10rpx;
  color: #006eff;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
  letter-spacing: 0;
}

.dialog-wrap {
  position: relative;
  padding: 0rpx 20rpx 30rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  display: -webkit-flex;
  width: 100%;
  flex-direction: column;
  -webkit-flex-direction: column;
}

.dialog-part {
  position: absolute;
  width: 100%;
  top: 60rpx;
  bottom: 125rpx;
  z-index:1;
}

